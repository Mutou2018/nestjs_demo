    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug nest",
            "type": "node",
            "runtimeExecutable": "npm",
            "args": [
                "run",
                "start:dev"
            ],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Attach",
            "port": 9229,
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/debug-test/index.js",
            "request": "launch",
            "stopOnEntry": true,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        }
    ]
}